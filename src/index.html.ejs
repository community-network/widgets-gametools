        <meta property="og:url" content="https://bandofbrothers.site/" />
        <meta property="og:site_name" content="bandofbrothers.site" />

        <!-- Bundle -->
        <% if (webpackConfig.mode == 'production') { %>
        <script defer src="/js/bundle.gametools-prod-edge.min.js"></script>
        <% } else { %>
        <script defer src="/main.js"></script>
        <% } %>

    </head>
    <body>
        <div id="root"></div>

