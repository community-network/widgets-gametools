{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
  },
  "sideEffects": "*.scss",
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm clean-dist && webpack --config=configs/webpack/prod.js --progress",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "pnpm start-dev --progress",
    "start-dev": "webpack serve --config=configs/webpack/dev.js --port 3001",
    "start-prod": "pnpm build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.8.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "browserslist": "^4.26.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "express": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "lightningcss": "^1.30.2",
    "prettier": "^3.6.2",
    "react-compiler-webpack": "^0.2.1",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "sharp": "^0.34.4",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.90.5",
    "copy-webpack-plugin": "^13.0.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "mini-css-extract-plugin": "^2.9.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.0.1",
    "react-router": "^7.9.4",
    "regenerator-runtime": "^0.14.1",
    "workbox-webpack-plugin": "^7.3.0"
  }
}